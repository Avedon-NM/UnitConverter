<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/addConversionType"
        android:layout_width="310dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="112dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="93dp"
        android:layout_height="36dp"
        android:gravity="left"
        android:text="Formula"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="275dp" />

    <EditText
        android:id="@+id/addFormula"
        android:layout_width="308dp"
        android:layout_height="41dp"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="282dp"
        tools:layout_editor_absoluteY="270dp" />

    <Button
        android:id="@+id/addConversionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add Conversion"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="328dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="93dp"
        android:layout_height="35dp"
        android:gravity="left"
        android:text="Unit 1"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="155dp" />

    <EditText
        android:id="@+id/addUnit1"
        android:layout_width="310dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="163dp" />

    <EditText
        android:id="@+id/addUnit2"
        android:layout_width="310dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="214dp" />

    <Button
        android:id="@+id/clearButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Clear Button"
        tools:layout_editor_absoluteX="412dp"
        tools:layout_editor_absoluteY="328dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="117dp"
        android:layout_height="31dp"
        android:gravity="left"
        android:text="Conversion Type"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="112dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="95dp"
        android:layout_height="38dp"
        android:gravity="left"
        android:text="Unit 2"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="214dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
